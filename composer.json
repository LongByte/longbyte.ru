{
    "name": "longbyte/site",
    "description": "longbyte",
    "version": "1.0.0",
    "config": {
        "vendor-dir": "./local/vendor",
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require": {
        "php": ">=8.2",
        "longbyte/api": "v3.1.*",
        "telegram-bot/api": "dev-master"
    },
    "minimum-stability": "dev",
    "extra": {
        "installer-paths": {
            "./local/php_interface/lib/Api/Core": [
                "longbyte/api"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.vdstech.ru:longbyte/longbyte.api.bx.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:TelegramBot/Api.git"
        }
    ]
}
